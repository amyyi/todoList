  "scripts": {
    "build-dev": "node ./node_modules/better-npm-run build-dev",
    "build-stage": "node ./node_modules/better-npm-run build-stage",
    "build-production": "node ./node_modules/better-npm-run build-production",
    "start-mock-api-local": "node ./node_modules/better-npm-run start-mock-api-local",
    "local": "concurrently --kill-others \"npm run start-mock-api-local\" \"node ./node_modules/better-npm-run local\""
  },
  "betterScripts": {
    "build-dev": {
      "command": "gulp build",
      "env": {
        "ENV": "dev"
      }
    },

    "local": {
      "command": "gulp serve:local",
      "env": {
        "ENV": "local"
      }
    },
    "start-mock-api-local": {
      "command": "node ./node_modules/supervisor/lib/cli-wrapper.js -w ./mockData, ./mockApi, ./server/mockapi.server.js",
      "env": {
        "ENV": "local",
        "NODE_PATH": "./mockApi"
      }
    },
    "e2e-dev": {
      "command": "node ./test/e2e/runner.js",
      "env": {
        "NODE_ENV": "testing"
      }
    }
  }
